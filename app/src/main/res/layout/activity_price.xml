<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:scaleType="centerCrop"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/wszoclbalk2"
        tools:layout_editor_absoluteX="0dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="411dp"
        android:layout_height="383dp"
        android:alpha="0.7"
        android:background="#100C0C"
        android:scaleType="centerCrop"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/bagblack"
        tools:layout_editor_absoluteY="348dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="412dp"
        android:layout_height="106dp"
        android:alpha="0.7"
        android:background="#00100C0C"
        android:scaleType="centerCrop"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/bagblack"
        tools:layout_editor_absoluteY="130dp" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="106dp"
        android:layout_height="116dp"
        android:background="@android:color/transparent"
        android:padding="0sp"
        android:scaleType="centerCrop"
        app:srcCompat="@drawable/human"
        tools:layout_editor_absoluteX="270dp"
        tools:layout_editor_absoluteY="404dp" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="96dp"
        android:layout_height="104dp"
        android:background="@android:color/transparent"
        android:padding="0sp"
        android:scaleType="centerCrop"
        app:srcCompat="@drawable/cal"
        tools:layout_editor_absoluteX="26dp"
        tools:layout_editor_absoluteY="560dp" />

    <ImageButton
        android:id="@+id/price"
        android:layout_width="96dp"
        android:layout_height="104dp"
        android:background="@android:color/transparent"
        android:padding="0sp"
        android:scaleType="centerCrop"
        app:srcCompat="@drawable/price_select"
        tools:layout_editor_absoluteX="26dp"
        tools:layout_editor_absoluteY="418dp" />

    <ImageButton
        android:id="@+id/disposar"
        android:layout_width="104dp"
        android:layout_height="107dp"
        android:background="@android:color/transparent"
        android:padding="0sp"
        android:scaleType="centerCrop"
        app:srcCompat="@drawable/rice"
        tools:layout_editor_absoluteX="149dp"
        tools:layout_editor_absoluteY="415dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="농산물 가격 예측"
        android:textColor="@color/white"
        tools:layout_editor_absoluteX="26dp"
        tools:layout_editor_absoluteY="522dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="산지 폐기"
        android:textColor="@color/white"
        tools:layout_editor_absoluteX="178dp"
        tools:layout_editor_absoluteY="522dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="계약 중계"
        android:textColor="@color/white"
        tools:layout_editor_absoluteX="302dp"
        tools:layout_editor_absoluteY="520dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="농업 정책 달력"
        android:textColor="@color/white"
        tools:layout_editor_absoluteX="26dp"
        tools:layout_editor_absoluteY="664dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="200dp"
        android:layout_height="192dp"
        app:srcCompat="@drawable/onion"
        tools:layout_editor_absoluteX="26dp"
        tools:layout_editor_absoluteY="86dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="<세부지표>"
        android:textColor="@color/white"
        android:textSize="25dp"
        tools:layout_editor_absoluteX="146dp"
        tools:layout_editor_absoluteY="52dp" />


</androidx.constraintlayout.widget.ConstraintLayout>